{
    "name": "dealer4dealer/module-substituteorders",
    "description": "A module so Magento saves orders from multiple sources in one table, overrides the default order list in customer backend. ",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Mage2Gen",
            "email": "info@mage2gen.com"
        },
        {
            "name": "Derrick Heesbeen",
            "email": "derrick@experius.nl"
        },
        {
            "name": "Daniel Oldermsa",
            "email": "d.oldersma@dealer4dealer.nl"
        }
    ],
    "minimum-stability": "stable",
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "autoload": {
        "psr-4": {
            "Dealer4Dealer\\SubstituteOrders\\": "src"
        },
        "files": [
            "src/registration.php"
        ]
    },
    "require": {
        "php": "~8.1|~8.2",
        "magento/framework": "^103.0",
        "magento/module-backend": "^102.0",
        "magento/module-customer": "^103.0",
        "magento/module-eav": "^102.1",
        "magento/module-sales": "^103.0",
        "magento/module-store": "^101.1",
        "magento/module-ui": "^101.2"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "youwe/coding-standard-phpstorm": true,
            "phpro/grumphp-shim": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "youwe/testing-suite": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "youwe/testing-suite": "^2.17"
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/grumphp.yml",
            "/pdepend.xml",
            "/phpstan.neon",
            "/phpunit.xml",
            "/phpcs.xml",
            "/phpmd.xml",
            "/tests"
        ]
    }
}
